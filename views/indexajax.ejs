<!DOCTYPE html>
<html>

<head>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>

  <title>Agenda Parser</title>
</head>

<body>

  <form>
    <input type="text" placeholder="formato 00" id="day">
    <input type="text" placeholder="formato 00" id="month">
    <input type="text" placeholder="formato 0000" id="year">
    <button id="submit1">Teste</button>
  </form>
  <script type='text/javascript'>
    document.getElementById('submit1').addEventListener('click', function() {
      date = {day:(document.getElementById('day').value), month:(document.getElementById('month').value),year:(document.getElementById('year').value)}
      alert(JSON.stringify(date))
      $.post('/dateAgenda',
      {
        data: (JSON.stringify(date)),
        dataType: 'json',
      }).done(function(data){
        alert('sucesso')
      })
    })
  </script>

</body>
</html>